{
  "manifest_version": 2,
  "name": "Extension template",
  "description": "React & Chrome template",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "React/Chrome Template",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ]
}